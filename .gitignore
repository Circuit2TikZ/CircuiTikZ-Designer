/.npm/

# Optional eslint cache
/.eslintcache

# Optional stylelint cache
/.stylelintcache

# Optional REPL history
/.node_repl_history

# Output of 'npm pack'
/*.tgz

# Yarn Integrity file
/.yarn-integrity

# dotenv environment variable files
/.env
/.env.development.local
/.env.test.local
/.env.production.local
/.env.local

# parcel-bundler cache (https://parceljs.org/)
/.cache
/.parcel-cache
/dist/

